	<script type="text/javascript">
		$(document).ready(function () {
			$('.group-chat-box .items-mess').slimScroll({
		        height: '350px'
		    });
			
			$('.user-login-link, .user-signup-link').on('click', function (e) {
				e.preventDefault();
				if ( $(this).hasClass('user-login-link') ) {
					$('a[href=#tab-signin]').trigger('click');
				}else {
					$('a[href=#tab-signup]').trigger('click');
				}
				$('#popup-login').modal('show');
			});
			
			$('.link-login-b, .link-regis-b').on('click', function (e) {
				e.preventDefault();
				if ( $(this).hasClass('link-login-b') ) {
					$('a[href=#tab-signin]').trigger('click');
				}else {
					$('a[href=#tab-signup]').trigger('click');
				}
			});
		});
	</script>
